{% extends "layout.html.twig" %}

{% block pagename %}
Create User
{% endblock %}

{% block content %}

  <img src="{{ asset('lib/img/Serpico.png') }}" alt="Logo Serpico" />
    {{ form_start(form) }}


  <div class="row">
    <div class="input-field col s10">
      <input disabled="disabled" value="Organisation" id="disabled" type="text" class="validate">
      <label for="disabled">Disabled</label>
    </div>
    <div class="row">
      <div class="input-field col s10">

          {{ form_widget(form.firstname) }}
          {{ form_label(form.firstname) }}
          {{ form_errors(form.firstname) }}
      </div>
    <div class="row">
      <div class="input-field col s10">

          {{ form_widget(form.lastname) }}
          {{ form_label(form.lastname) }}
          {{ form_errors(form.lastname) }}
      </div>
    <div class="row">
      <div class="input-field col s10">

          {{ form_widget(form.email) }}
          {{ form_label(form.email) }}
          {{ form_errors(form.email) }}
      </div>
    </div>
      <div class="row">
        <div class="input-field col s10">

            {{ form_widget(form.positionName) }}
            {{ form_label(form.positionName) }}
            {{ form_errors(form.positionName) }}
        </div>
      </div>
      <div class="row">
        <div class="input-field col s10">
            {{ form_widget(form.positionWeight) }}
            {{ form_label(form.positionWeight) }}
            {{ form_errors(form.positionWeight) }}
        </div>
      </div>
  </div>
  <div class="row">
    <div class="button-field col s10">
      {{ form_row(form.submit) }}
    </div>
  </div>

  {{ form_end(form)}}
{% endblock %}
