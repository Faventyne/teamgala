<!DOCTYPE html>
<html>
    <head>
        <title>{% block title '' %} - Serpico</title>

        <!-- CSS custom -->
        <link href="{{ asset('css/style.css') }}" rel="stylesheet" type="text/css" />
        <!-- CSS Materialize -->
        <link rel="stylesheet" href="{{ asset('lib/materialize/css/materialize.css') }}">
        <!-- CSS JQuery UI-->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.css">
        <!-- Materialize icons  -->
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

        <script type="text/javascript">
            var _gaq = _gaq || [];
            _gaq.push(['_setAccount', 'UA-XXXXXXXX-X']);
            _gaq.push(['_trackPageview']);

            (function() {
                var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
            })();
        </script>
    </head>
    <body>

      <header id="wesh">
        <a href="{{ path('home') }}"><i id="returnMenu" class="material-icons">home</i></a>
        <a href="#"><i id="returnPage" class="material-icons">chevron_left</i></a>
        <a href="#"><i id="logOutButton" class="material-icons">person</i></a>
        <div class="currentPage">
          {% block pagename %}{% endblock %}
           {% block username %}{% endblock %}
        </div>

      </header>

        {% block content %}{% endblock %}
        <!-- CDN : jquery -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js" ></script>
        <!-- CDN : script Materialize -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
        <!-- CDN JQuery UI -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.js"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
